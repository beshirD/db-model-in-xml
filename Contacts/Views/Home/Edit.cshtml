@{
    ViewBag.Title = "About";
}

    <form action="/Home/Update" method="put">
        <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" name="name" value="@Model["Name"].InnerText">
        </div>
        <div class="form-group">
            <label for="phoneno">Phoneno</label>
            <input type="text" class="form-control" id="PhoneNo" name="PhoneNo" value="@Model["PhoneNo"].InnerText">
        </div>
        <div class="form-group">
            <label for="email">Email</label>
            <input type="text" class="form-control" id="email" name="email" value="@Model["Email"].InnerText">
        </div>
        <div class="form-group">
            <label for="houseno">HouseNo</label>
            <input type="text" class="form-control" id="houseno" name="houseno" value="@Model["HouseNo"].InnerText">
        </div>
        <button type="submit" class="btn btn-primary">Update</button>
    </form>
